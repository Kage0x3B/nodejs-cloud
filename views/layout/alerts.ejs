
<% if (err.length > 0) { %>
  <%= console.log(new Date(),err,user.username) %>
  <div class="alert1 alert alert-danger text-center">
    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
    <%= err %></div>
<% } %>

<% if (msg.length > 0) { %>
  <%= console.log(new Date(),msg,user.username) %>
  <div class="alert1 alert alert-info text-center">
  <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
  <%= msg %></div>
<% } %>
